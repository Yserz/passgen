  "lint-staged": {
    "*.js": [
      "eslint -c .eslintrc.yaml --fix",
      "git add"
    ],
    "*.json": [
      "prettier --write",
      "git add"
    ],
    "*.md": [
      "prettier --write",
      "git add"
    ],
    "src/script/**/*.{ts,tsx}": [
      "tslint --config tslint.json --project tsconfig.json --fix",
      "git add"
    ],
    "server/**/*.{ts,tsx}": [
      "cd server && tslint --config server/tslint.json",
      "git add"
    ]
  },
  "name": "@passgen/webapp",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/Yserz/passgen/master/webapp"
  },
  "scripts": {
    "bundle:prod": "cross-env-shell NODE_ENV=production yarn clear && yarn webpack",
    "bundle:staging": "cross-env-shell BABEL_ENV=development NODE_ENV=production yarn clear && yarn webpack",
    "clear": "rimraf dist",
    "start": "webpack-dev-server --config webpack.config.dev.js",
    "test": "yarn test:types && yarn clear && webpack --config webpack.config.test.js && karma start karma.conf.js",
    "test:types": "tsc --noEmit"
  },
  "version": "0.0.1"
