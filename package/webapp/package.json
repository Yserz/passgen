      "git add"
    ],
    "*.json": [
      "prettier --write",
      "git add"
    ],
    "*.md": [
      "prettier --write",
      "git add"
    ],
    "src/script/**/*.{ts,tsx}": [
      "tslint --config tslint.json --project tsconfig.json --fix",
      "git add"
